<UserControl x:Class="CTTManagement.FormVCheckBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CTTManagement"
             xmlns:viewmodel="clr-namespace:CTTManagement.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="192">
    <Grid d:DataContext="{d:DesignInstance {x:Type viewmodel:MainAppVM}}">
       
        <!--FormVCheckBoxes -->
        <DockPanel>
            <Label DockPanel.Dock="Top" Content="FormV Table Settings:"/>
            <ListBox ItemsSource="{Binding FormVCheckBoxes}"/>
        </DockPanel>
       

    </Grid>
</UserControl>
